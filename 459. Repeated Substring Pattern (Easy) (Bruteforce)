class Solution {
public:
    bool repeatedSubstringPattern(string s) {
        for(int i = 1; i < s.size()/2+1; i++)
        {
            if(s.size() % i != 0)
                continue;
            string sub = s.substr(0, i);
            for(int j = 1; j < s.size()/i; j++)
                if(s.substr(j*i, i) != sub)
                    goto A;
            return true;  
            A:;
        }
        return false;
    }
};
